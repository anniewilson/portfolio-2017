      meta(name='description', content=summary)
      meta(name='twitter:card', content=summary_large_image)
      meta(name='twitter:site', content='@broccoliwilson')
      meta(name='twitter:title', content=title)
      meta(name='twitter:description', content=summary)
      meta(name='twitter:creator', content='@broccoliwilson')
      meta(name='twitter:image', content=metaImage)
      meta(property='og:title', content=title)
      meta(property='og:type', content='article')
      meta(property='og:url', content=url)
      meta(property='og:image', content=metaImage)
      meta(property='og:description', content=summary)
      meta(property='og:site_name', content='Annie Wilson Portfolio')
        
  body(class=bodyclass)#pageTop

    //- SVG Sprite
    svg(xmlns='http://www.w3.org/2000/svg').hidden
      symbol#icon--arrow-download(viewbox='0 0 16 16')
        title arrow-download
        path(d='M11.5 8.5L8 12 4.5 8.5M8 12V0')
        path(d='M.5 7.5v7h15v-7')
      symbol#icon--arrow-right-bent(viewbox='0 0 16 12')
        title arrow-right-bent
        path(d='M11.5 4.5L15 8l-3.5 3.5')
        path(d='M15 8H1V0')
      symbol#icon--arrow-right(viewbox='0 0 16 8')
        title arrow-right
        path(d='M11.5.5L15 4l-3.5 3.5M15 4H0')
      symbol#icon--arrow-up(viewbox='0 0 32 50')
        title arrow-up
        path(d='M1 15.83l15-15 15 15m-15-15v49')
      symbol#icon--blob(viewbox='0 0 44 44')
        title blob
        path.st0(d='M1.8 11.8c-5.6 11.8 8.9 29.7 24 29.7S47.3 22.3 41 11.8C36 3.3 9.4-4 1.8 11.8z')
   
    //- Global Header
    header.header

      //- Logo
      .header__home-link
        a.logo(href='/')
          svg.logo__svg(role='img', aria-labelledby='title')
            title Arrow up icon
            use(xlink:href='#icon--blob')
          | Annie Wilson

      //- Primary Nav
      nav.header__nav.primary-nav
        ul
          each item in locals.nav
            li.primary-nav__item
              a.primary-nav__link(class=item.cssClass, href=item.url)= item.name

    //- Global Main Content Area
    #barba-wrapper
      .barba-container
        main.main
          div.l-centred

            //- Header
            block header

            //- Content
            block content
              h2 Welcome to zombocom!

    //- Global Footer
    block footer
      footer#contact.footer
        .l-centred
          h2.alt.footer__cta Let’s work together.
          p
            | I thrive in small teams with big ambitions and passion for days. 
            br
            | If that sounds like you, hit me up for a chat!
          p.alt
            a(href='mailto:annie@broccoliwilson.com') annie@broccoliwilson.com

          //- Footer Social Nav
          nav.social-nav
            ul
              li.social-nav__item
                a.social-nav__link(href='https://www.linkedin.com/in/broccoli-wilson/' target="_blank") LinkedIn
              li.social-nav__item
                a.social-nav__link(href='https://github.com/anniewilson/' target="_blank") Github
              li.social-nav__item
                a.social-nav__link(href='https://medium.com/@BroccoliWilson/' target="_blank") Medium
              li.social-nav__item
                a.social-nav__link(href='https://www.instagram.com/broccoliwilson/' target="_blank") Instagram
              li.social-nav__item
                a.social-nav__link(href='https://twitter.com/BroccoliWilson' target="_blank") Twitter

          //- Footer 'Page top' Link
          a.footer__top(href='#pageTop')
            svg.icon.icon--arrow-up(role='img', aria-labelledby='title')
              title Arrow up icon
              use(xlink:href='#icon--arrow-up')
      
      script(src='https://code.jquery.com/jquery-3.2.1.min.js', integrity='sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=', crossorigin='anonymous')
      script(src='/js/all.min.js', type='text/javascript')