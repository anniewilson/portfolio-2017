doctype html
block vars
  - var bodyclass = null;
  - var summary = 'I’m an Australian designer in Sydney. I’m currently working at This Little Duck, where I lead the design direction on our projects. Some of our clients include UNSW, ABC, SBS, Garage Sale Trail and Inner West Council.';
  - var metaImage = 'http://broccoliwilson.com/images/meta-image.jpg';


html(lang='en')
  head
    block head
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
      meta(name='viewport', content='width=device-width, initial-scale=1.0')
      title
        block title
          = locals.name
      link(rel='alternate', href=locals.url+'/feed.xml', type='application/rss+xml', title=locals.description)
      link(rel='stylesheet', href='//fonts.googleapis.com/css?family=Roboto+Mono:300,400,500')
      link(rel='stylesheet', href=contents.css['style.min.css'].url)
      | !{ livereloadScript }
      link(rel='shortcut icon', href='/images/favicon.ico')
       
